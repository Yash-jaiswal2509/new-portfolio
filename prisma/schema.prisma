generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id            Int @id @default(autoincrement())
  name          String
  imageUrl      String
  description   String
  githubUrl     String
  projectUrl    String
  createdAt     DateTime @default(now())
}
